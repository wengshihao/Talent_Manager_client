<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ChatdetilActivity"
    android:background="@color/barcolor">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/imageView11"
                    android:layout_width="368dp"
                    android:layout_height="786dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.372"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.0"
                    app:srcCompat="@drawable/ic_talent_picture9" />

                <TextView

                    android:id="@+id/detailskill"
                    android:layout_width="275dp"
                    android:layout_height="32dp"
                    android:layout_marginStart="52dp"
                    android:layout_marginTop="268dp"
                    android:text="TextView"
                    android:textSize="20sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:singleLine="false"
                    android:scrollbars="vertical"
                    android:id="@+id/detailjixiao"
                    android:layout_width="336dp"
                    android:layout_height="267dp"
                    android:layout_marginStart="32dp"
                    android:layout_marginTop="356dp"
                    android:background="@drawable/ic_talent_picture12"
                    android:text="TextView"
                    android:textSize="20sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/imageView12"
                    android:layout_width="102dp"
                    android:layout_height="113dp"
                    android:layout_marginStart="64dp"
                    android:layout_marginTop="44dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@drawable/ic_talent_man6" />

                <TextView
                    android:id="@+id/detailname"
                    android:layout_width="161dp"
                    android:layout_height="26dp"
                    android:layout_marginStart="188dp"
                    android:layout_marginTop="44dp"
                    android:text="TextView"
                    android:textSize="20sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/detailsex"
                    android:layout_width="161dp"
                    android:layout_height="26dp"
                    android:layout_marginStart="188dp"
                    android:layout_marginTop="84dp"
                    android:text="TextView"
                    android:textSize="20sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/detailage"
                    android:layout_width="161dp"
                    android:layout_height="26dp"
                    android:layout_marginStart="188dp"
                    android:layout_marginTop="120dp"
                    android:text="TextView"
                    android:textSize="20sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/detailcompany"
                    android:layout_width="57dp"
                    android:layout_height="29dp"
                    android:layout_marginStart="32dp"
                    android:layout_marginTop="40dp"
                    android:text="TextView"
                    android:textSize="20sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/detailcompany2"
                    android:layout_width="137dp"
                    android:layout_height="33dp"
                    android:layout_marginStart="52dp"
                    android:layout_marginTop="312dp"
                    android:text="项目经历："
                    android:textSize="20sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/detailgangwei"
                    android:layout_width="279dp"
                    android:layout_height="30dp"
                    android:layout_marginStart="52dp"
                    android:layout_marginTop="176dp"
                    android:text="TextView"
                    android:textSize="20sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/detilxinzi"
                    android:layout_width="279dp"
                    android:layout_height="30dp"
                    android:layout_marginStart="52dp"
                    android:layout_marginTop="220dp"
                    android:text="TextView"
                    android:textSize="20sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <Button
                    android:id="@+id/onlinebtn"
                    android:layout_width="137dp"
                    android:textColor="@color/white"
                    android:layout_height="47dp"

                    android:layout_marginStart="128dp"
                    android:layout_marginBottom="104dp"
                    android:background="@drawable/ic_talent_picture13"
                    android:text="重新导入简历"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>